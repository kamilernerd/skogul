{
  "receivers": {
    "api": {
      "type": "http",
      "address": "[::1]:8080",
      "log204ok": true,
      "handlers": {
        "/test": "jsontemplating"
      },
      "auth": {
        "/test": { "username": "lol", "password": "kek" }
      }
    }
  },
  "handlers": {
    "loghandler": {
      "parser": "skogul",
      "sender": "log-batch"
    },
    "jsontemplating": {
      "parser": "skogul",
      "transformers": [],
      "sender": "count"
    },
    "debugger": {
      "parser": "skogul",
      "transformers": [],
      "sender": "print"
    }
  },
  "senders": {
		"log-batch": {
			"type": "batch",
			"next": "forward"
		},
		"forward": {
			"type": "sleep",
			"base": "1ms",
			"maxdelay": "1s",
			"next": "print"
		},
    "batch": {
      "type": "batch",
      "threshold": 1000,
      "next": "count"
    },
    "count": {
      "type": "null",
      "stats": "debugger",
      "next": "print"
    }
  }
}
